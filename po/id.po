#: xml/release-notes.xml:247(para)
msgid ""
"<package>emerillon</package>: Replaced by <package>gnome-maps</package>."
msgstr ""
"<package>emerillon</package>: Diganti dengan <package>gnome-maps</package>."

#: xml/release-notes.xml:253(para)
msgid ""
"<package>gnome-system-log</package>: Replaced by <package>gnome-logs</"
"package>."
msgstr ""
"<package>gnome-system-log</package>: Diganti dengan <package>gnome-logs</"
"package>."

#: xml/release-notes.xml:259(para)
msgid "<package>hawk</package>: Replaced by <package>hawk2</package>."
msgstr "<package>hawk</package>: Diganti dengan <package>hawk2</package>."

#: xml/release-notes.xml:264(para)
msgid "<package>ksnapshot</package>: Replaced by <package>spectacle</package>."
msgstr ""
"<package>ksnapshot</package>: Diganti dengan <package>spectacle</package>."

#: xml/release-notes.xml:270(para)
msgid ""
"<package>labplot</package>: Labplot has been replaced by its Qt5 version, "
