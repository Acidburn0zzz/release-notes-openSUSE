msgstr "<package>dropbear</package>: Dikeluarkan karena tidak ada kelebihannya dibanding <package>openssh</package>."

#: xml/release-notes.xml:191(para)
msgid "<package>gnome-system-log</package>: Replaced by <package>gnome-logs</package>."
msgstr "<package>gnome-system-log</package>: Diganti dengan <package>gnome-logs</package>."

